Proceso problema4
	// Algoritmo para hallar la hipotenusa por Pitágoras
	// h^2 = (a^2 + b^2)
	Definir hipotenusa, cateto1, cateto2 Como Real;
	// Procesos
	Escribir 'Ingrese el valor del cateto 1: ';
	Leer cateto1;
	Escribir 'Ingrese el valor del cateto 2: ';
	Leer cateto2;
	hipotenusa <- raiz((cateto1^2)+(cateto2^2));
	Escribir 'La hipotenusa es de: ', hipotenusa;
FinProceso
